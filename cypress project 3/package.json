    "cypress-failed-log": "^2.7.0",
    "cypress-image-snapshot": "^3.1.1",
    "prettier": "^2.0.5"
  },
  "devDependencies": {
    "cypress": "^4.12.1",
    "dark": "0.0.2-2"
  }
}
